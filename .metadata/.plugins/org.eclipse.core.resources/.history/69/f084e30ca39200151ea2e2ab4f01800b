package Yatzy;

public class Controller {
	public Model model;
	public View view;
	
	public Controller (Model model, View view){
		this.model = model;
		this.view = view;
		
		Start(this.model, this.view);
	}
	
	public void Start(Model model, View view){
		view.GetStartMenu();
		int choice = view.InputStartMenu();
	}
}
